journal = {Blood},
keywords = {*Vaccines,Antigens, CD/biosynthesis,Antigens, CD45/biosynthesis,Antigens, Differentiation, T-Lymphocyte/biosynthes,Biotinylation,CD8-Positive T-Lymphocytes/*virology,Cell Division,Cell Separation,Enzyme-Linked Immunosorbent Assay,Flow Cytometry,HIV-1/*metabolism,HLA-A2 Antigen/*metabolism,Herpesvirus 8, Human/*metabolism,Humans,Immunosuppressive Agents/pharmacology,Interferon-gamma/metabolism,Lectins, C-Type,Leukocytes, Mononuclear/metabolism,Major Histocompatibility Complex,Models, Biological,Phenotype},
