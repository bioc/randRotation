journal = {European Journal of Pharmacology},
keywords = {ATPase,Caloxin 2A1,Erythrocyte,Homeostasis,Signal transduction},
number = {1-3},
pages = {1--6},
pmid = {15680248},
title = {{A novel plasma membrane Ca2+-pump inhibitor: Caloxin 1A1}},
