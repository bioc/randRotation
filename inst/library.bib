journal = {Journal of the American Chemical Society},
keywords = {Animals,Antineoplastic Agents,Antineoplastic Agents: chemistry,Antineoplastic Agents: pharmacology,Cell Line, Tumor,Cell Proliferation,Cell Proliferation: drug effects,Cell Survival,Cell Survival: drug effects,Dose-Response Relationship, Drug,Drug Carriers,Drug Carriers: chemistry,Drug Delivery Systems,Drug Screening Assays, Antitumor,Ferrous Compounds,Ferrous Compounds: chemistry,Humans,Hydrogen-Ion Concentration,Macromolecular Substances,Macromolecular Substances: chemistry,Mice,Mitoxantrone,Mitoxantrone: chemistry,Mitoxantrone: pharmacology,NIH 3T3 Cells,Quaternary Ammonium Compounds,Quaternary Ammonium Compounds: chemistry,Solubility,Structure-Activity Relationship,Water,Water: chemistry},
