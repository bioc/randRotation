@article{Storey2003,
abstract = {With the increase in genomewide experiments and the sequencing of multiple$\backslash$n genomes, the analysis of large data sets has become commonplace in biology. It$\backslash$n is often the case that thousands of features in a genomewide data set are$\backslash$n tested against some null hypothesis, where a number of features are expected$\backslash$n to be significant. Here we propose an approach to measuring statistical$\backslash$n significance in these genomewide studies based on the concept of the false$\backslash$n discovery rate. This approach offers a sensible balance between the number of$\backslash$n true and false positives that is automatically calibrated and easily$\backslash$n interpreted. In doing so, a measure of statistical significance called the$\backslash$n q value is associated with each tested feature. The q value$\backslash$n is similar to the well known p value, except it is a measure of$\backslash$n significance in terms of the false discovery rate rather than the false$\backslash$n positive rate. Our approach avoids a flood of false positive results, while$\backslash$n offering a more liberal criterion than what has been used in genome scans for$\backslash$n linkage.},
author = {Storey, John D. and Tibshirani, Robert},
doi = {10.1073/pnas.1530509100},
file = {:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Storey, Tibshirani{\_}2003{\_}Statistical significance for genomewide studies.pdf:pdf},
