issn = {09547894},
journal = {Clinical {\&} Experimental Allergy},
keywords = {anisakis allergy,b cell epitopes,epitope mapping,inhibitors,kunitz-type serine protein,recombinant ani s 1,t cell epitopes},
number = {5},
pages = {994--1005},
pmid = {25495594},
title = {{Relationships between T cell and IgE/IgG4 epitopes of the Anisakis simplex major allergen Ani s 1}},
