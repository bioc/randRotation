issn = {1095-9157},
journal = {Journal of autoimmunity},
keywords = {Autoimmune Diseases,Autoimmune Diseases: immunology,Cardiovascular Diseases,Cardiovascular Diseases: etiology,Cardiovascular Diseases: immunology,Cytokines,Cytokines: immunology,Endocytosis,Endocytosis: immunology,Humans,Hypersensitivity,Hypersensitivity: immunology,Immunity, Cellular,Inflammation Mediators,Inflammation Mediators: adverse effects,Inflammation Mediators: chemistry,Inflammation Mediators: classification,Nanoparticles,Nanoparticles: adverse effects,Nanoparticles: chemistry,Nanoparticles: classification,Neoplasms,Neoplasms: immunology,Oxidative Stress,Oxidative Stress: immunology,Self Tolerance},
month = {may},
