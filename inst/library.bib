abstract = {RNA-seq is now the technology of choice for genome-wide$\backslash$ndifferential gene expression experiments, but it is not clear how$\backslash$nmany biological replicates are needed to ensure valid biological$\backslash$ninterpretation of the results or which statistical tools are best$\backslash$nfor analyzing the data. An RNA-seq experiment with 48 biological$\backslash$nreplicates in each of two conditions was performed to answer$\backslash$nthese questions and provide guidelines for experimental design.$\backslash$nWith three biological replicates, eight of the 11 tools evaluated$\backslash$nfound only 20{\%}–40{\%} of the significantly differentially$\backslash$nexpressed (SDE) genes identified with the full set of 42 clean$\backslash$nreplicates. This rises to {\textgreater}85{\%} for the subset of SDE genes$\backslash$nchanging in expression by more than fourfold. To achieve {\textgreater}85{\%}$\backslash$nfor all SDE genes regardless of fold change requires more than 20$\backslash$nbiological replicates. The same eight tools successfully control$\backslash$ntheir false discovery rate at ≲5{\%} for all numbers of replicates,$\backslash$nwhile the remaining three tools fail to control their FDR$\backslash$nadequately, particularly for low numbers of replicates. For$\backslash$nfuture RNA-seq experiments, these results suggest that more than$\backslash$nsix biological replicates should be used, rising to more than 12$\backslash$nwhen it is important to identify SDE genes for all fold changes.$\backslash$nIf less than 12 replicates are used, a superior combination of$\backslash$ntrue positive and false positive performances makes edgeR the$\backslash$nleading tool. For higher replicate numbers, minimizing false$\backslash$npositives is more important and DESeq marginally outperforms the$\backslash$nother tools.},
archivePrefix = {arXiv},
arxivId = {1505.02017},
author = {Schurch, Nicholas J and Schofield, Piet{\'{a}} and Gierli{\'{n}}ski, Marek and Cole, Christian and Sherstnev, Alexander and Singh, Vijender and Wrobel, Nicola and Gharbi, Karim and Simpson, Gordon$\backslash$nG and Owen-Hughes, Tom and Blaxter, Mark and Barton, Geoffrey J},
doi = {10.1261/rna.053959.115},
eprint = {1505.02017},
file = {:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Schurch et al.{\_}2016{\_}How many biological replicates are needed in an RNA-seqnexperiment and which differential expression tool should you.pdf:pdf},
isbn = {1355-8382, 1469-9001},
issn = {1355-8382},
journal = {Rna},
keywords = {benchmarking,differential expression,experimenta},
number = {10},
