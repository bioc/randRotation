pmid = {19931263},
publisher = {Elsevier Inc.},
title = {{Salivary Transcriptomic Biomarkers for Detection of Resectable Pancreatic Cancer}},
url = {http://dx.doi.org/10.1053/j.gastro.2009.11.010},
volume = {138},
year = {2010}
