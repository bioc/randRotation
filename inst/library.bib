isbn = {1874-3919},
issn = {18743919},
journal = {Journal of Proteomics},
keywords = {Autoantibody tumour biomarkers,Phage display,Protein microarrays,SEREX,SERPA,Tumour-associated antigens},
pages = {102--115},
pmid = {22842156},
publisher = {Elsevier B.V.},
title = {{The current status of cancer biomarker research using tumour-associated antigens for minimal invasive and early cancer diagnostics}},
url = {http://dx.doi.org/10.1016/j.jprot.2012.07.022},
volume = {76},
