pages = {2125--2138},
title = {{Limited tumor infiltration by activated T effector cells restricts the therapeutic activity of regulatory T cell depletion against established melanoma}},
url = {http://jem.rupress.org/content/205/9/2125.abstract},
volume = {205 },
year = {2008}
}
