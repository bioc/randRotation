issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {Alternative Splicing,Alternative Splicing: drug effects,Amino Acid Sequence,Apigenin,Apigenin: metabolism,Apigenin: pharmacology,Base Sequence,Biological Transport,Biological Transport: drug effects,Cell Line,Cell Membrane,Cell Membrane: metabolism,DNA,Diet,Enzyme Activation,Enzyme Activation: drug effects,Flavonoids,Flavonoids: metabolism,Flavonoids: pharmacology,GTP Phosphohydrolases,GTP Phosphohydrolases: genetics,GTP Phosphohydrolases: metabolism,HeLa Cells,Heterogeneous-Nuclear Ribonucleoprotein Group A-B,Heterogeneous-Nuclear Ribonucleoprotein Group A-B:,Humans,Messenger,Messenger: genetics,Messenger: metabolism,Molecular Sequence Data,Peptide Library,Protein Binding,Protein Multimerization,Protein Multimerization: drug effects,RNA,RNA Stability,RNA Stability: drug effects,RNA Transport,RNA Transport: drug effects,Sequence Analysis,Tumor},
