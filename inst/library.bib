file = {:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Weisser et al.{\_}2013{\_}An automated pipeline for high-throughput label-free quantitative proteomics.pdf:pdf},
isbn = {1535-3893},
