file = {:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/L. et al.{\_}2004{\_}Gene expression in the urinary bladder A common carcinoma in situ gene expression signature exists disregarding histopath.pdf:pdf},
issn = {0008-5472},
journal = {Cancer Research},
keywords = {DNA methylation,angiogenesis,article,bladder biopsy,bladder carcinoma,carcinoma in situ,chromosome 9,chromosome inactivation,controlled study,early cancer,gene cluster,gene expression profiling,histopathology,human,human tissue,priority journal,promoter region,transitional cell carcinoma,tumor invasion,urothelium},
