doi = {10.1007/BF02595811},
file = {:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Ge, Speed, Dudoit{\_}2003{\_}Resampling-based multiple testing for microarray data analysis.pdf:pdf},
