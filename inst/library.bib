@article{Bartlett2008,
abstract = {In this paper we have distinguished between the concepts$\backslash$n$\backslash$nof agreement and reliability. Neither parameter is superior$\backslash$n$\backslash$nto the other, as they describe different characteristics of the$\backslash$n$\backslash$nmeasurement process. The choice of what to report in a$\backslash$n$\backslash$nparticular study should be guided by how measurements$\backslash$n$\backslash$nare to be used in the future, and also by the fact that$\backslash$n$\backslash$nreaders may want to use a measurement method in adifferent population.$\backslash$nWe have highlighted the fact that$\backslash$n$\backslash$nthe reliability of a method depends not only on the size$\backslash$n$\backslash$nof the measurement errors but on the heterogeneity of$\backslash$n$\backslash$ntrue values in the population in which measurements are$\backslash$n$\backslash$nmade.Asmeasurement techniques potentially may be used$\backslash$n$\backslash$nin a variety of settings (e.g. clinical trials, screening) and$\backslash$n$\backslash$ndifferent populations, it is advisable to report estimates of$\backslash$n$\backslash$nbetween- and within-subject SDs.$\backslash$n$\backslash$nWe have outlined which methods we believe are appropriate$\backslash$n$\backslash$nfor the analysis of repeatability studies, method$\backslash$n$\backslash$ncomparison studies, and studies with measurements made$\backslash$n$\backslash$nby different observers or raters. In particular, we do not$\backslash$n$\backslash$nbelieve a single reliability coefficient should be used for$\backslash$n$\backslash$nmethod comparison studies. If the reliability of two methods$\backslash$n$\backslash$nare to be compared, each method�s reliability should$\backslash$n$\backslash$nbe estimated separately, by making at least two measurements$\backslash$n$\backslash$non each subject with each measurement method.$\backslash$n$\backslash$nWe have discussed how an association between paired$\backslash$n$\backslash$ndifferences and means may not necessarily be caused by$\backslash$n$\backslash$nchanging bias between two methods. Such an association$\backslash$n$\backslash$nmay also be caused by a difference in the methods�$\backslash$n$\backslash$nmeasurement error SDs, but with only one measurement$\backslash$n$\backslash$nper subject per method it is not possible to determine$\backslash$n$\backslash$nwhich of these is the cause. Method comparison studies$\backslash$n$\backslash$nshould therefore make at least two measurements per$\backslash$n$\backslash$nsubject per method. This permits an investigation of the$\backslash$n$\backslash$nsource of any association between paired differences and$\backslash$n$\backslash$nmeans for measurements made by the two methods, and$\backslash$n$\backslash$nalso allows the repeatability and reliability of eachmethod$\backslash$n$\backslash$nto be estimated.$\backslash$n$\backslash$nFinally, where measurements involve an observer or$\backslash$n$\backslash$nrater, measurement error studies must use an adequate$\backslash$n$\backslash$nnumber of observers if interest lies in making inferences$\backslash$n$\backslash$nabout a wider population of observers.},
author = {Bartlett, J W and Frost, C},
doi = {10.1002/uog.5256},
file = {:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Bartlett, Frost{\_}2008{\_}Reliability, repeatability and reproducibility analysis of measurement errors in continuous variables.pdf:pdf},
