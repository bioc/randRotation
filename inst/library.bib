issn = {00221759},
journal = {Journal of Immunological Methods},
keywords = {Data normalization,ELISA,IgG,Serum and plasma profiling,Tumor antigens,Validation},
number = {1-2},
pages = {50--58},
pmid = {19041653},
publisher = {Elsevier B.V.},
title = {{Seromic analysis of antibody responses in non-small cell lung cancer patients and healthy donors using conformational protein arrays}},
