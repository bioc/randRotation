isbn = {1573-3882},
issn = {15733890},
journal = {Metabolomics},
keywords = {Human saliva,LC–MS,Metabolomics,Multi-platform,NMR,Quantitative},
number = {6},
pages = {1864--1883},
title = {{The human saliva metabolome}},
volume = {11},
year = {2015}
}
