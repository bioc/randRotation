file = {:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Barry, Nobel, Wright{\_}2005{\_}Significance analysis of functional categories in gene expression studies A structured permutation approach.pdf:pdf},
