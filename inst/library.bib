isbn = {1535-9484 (Electronic)$\backslash$r1535-9476 (Linking)},
issn = {1535-9476},
journal = {Molecular {\&} Cellular Proteomics},
pages = {1790--1800},
pmid = {22984286},
title = {{High-resolution mapping of linear antibody epitopes using ultrahigh-density peptide microarrays}},
year = {2012}
}
@article{Chapman2012,
abstract = {Tumor-associated autoantibodies (AAbs) have been described in patients with lung cancer, and the EarlyCDT{\textregistered}-Lung test that measures such AAbs is available as an aid for the early detection of lung cancer in high-risk populations. Improvements in specificity would improve its cost-effectiveness, as well as reduce anxiety associated with false positive tests. Samples from 235 patients with newly diagnosed lung cancer and matched controls were measured for the presence of AAbs to a panel of six (p53, NY-ESO-1, CAGE, GBU4-5, Annexin I, and SOX2) or seven (p53, NY-ESO-1, CAGE, GBU4-5, SOX2, HuD, and MAGE A4) antigens. Data were assessed in relation to cancer type and stage. The sensitivity and specificity of these two panels were also compared in two prospective consecutive series of 776 and 836 individuals at an increased risk of developing lung cancer. The six-AAb panel gave a sensitivity of 39{\%} with a specificity of 89 {\%}, while the seven-AAb panel gave a sensitivity of 41 {\%} with a specificity of 91 {\%} which, once adjusted for occult cancers in the population, resulted in a specificity of 93 {\%}. Analysis of these AAb assays in the at-risk population confirmed that the seven-AAb panel resulted in a significant increase in the specificity of the test from 82 to 90 {\%}, with no significant change in sensitivity. The change from a six- to a seven-AAb assay can improve the specificity of the test and would result in a PPV of 1 in 8 and an overall accuracy of 92 {\%}.},
author = {Chapman, Caroline J. and Healey, Graham F. and Murray, Andrea and Boyle, Peter and Robertson, Chris and Peek, Laura J. and Allen, Jared and Thorpe, Alison J. and Hamilton-Fairley, Geoffrey and Parsy-Kowalska, Celine B. and MacDonald, Isabel K. and Jewell, William and Maddison, Paul and Robertson, John F R},
