title = {{Formation of unilamellar vesicles by repetitive freeze-thaw cycles: characterization by electron microscopy and 31P-nuclear magnetic resonance.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/10968210},
volume = {29},
