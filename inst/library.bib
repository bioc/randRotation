@article{Larsen2006,
abstract = {BACKGROUND: B-cell epitopes are the sites of molecules that are recognized by antibodies of the immune system. Knowledge of B-cell epitopes may be used in the design of vaccines and diagnostics tests. It is therefore of interest to develop improved methods for predicting B-cell epitopes. In this paper, we describe an improved method for predicting linear B-cell epitopes.$\backslash$n$\backslash$nRESULTS: In order to do this, three data sets of linear B-cell epitope annotated proteins were constructed. A data set was collected from the literature, another data set was extracted from the AntiJen database and a data sets of epitopes in the proteins of HIV was collected from the Los Alamos HIV database. An unbiased validation of the methods was made by testing on data sets on which they were neither trained nor optimized on. We have measured the performance in a non-parametric way by constructing ROC-curves.$\backslash$n$\backslash$nCONCLUSION: The best single method for predicting linear B-cell epitopes is the hidden Markov model. Combining the hidden Markov model with one of the best propensity scale methods, we obtained the BepiPred method. When tested on the validation data set this method performs significantly better than any of the other methods tested. The server and data sets are publicly available at http://www.cbs.dtu.dk/services/BepiPred.},
author = {Larsen, Jens Erik Pontoppidan and Lund, Ole and Nielsen, Morten},
doi = {10.1186/1745-7580-2-2},
isbn = {1745-7580},
issn = {1745-7580},
journal = {Immunome research},
pages = {2},
pmid = {16635264},
title = {{Improved method for predicting linear B-cell epitopes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1479323{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {2},
year = {2006}
}
@article{Gibbons2018,
abstract = {High-throughput data generation platforms, like mass-spectrometry, microarrays, and second-generation sequencing are susceptible to batch effects due to run-to-run variation in reagents, equipment, protocols, or personnel. Currently, batch correction methods are not commonly applied to microbiome sequencing datasets. In this paper, we compare different batch-correction methods applied to microbiome case-control studies. We introduce a model-free normalization procedure where features (i.e. bacterial taxa) in case samples are converted to percentiles of the equivalent features in control samples within a study prior to pooling data across studies. We look at how this percentile-normalization method compares to traditional meta-analysis methods for combining independent p-values and to limma and ComBat, widely used batch-correction models developed for RNA microarray data. Overall, we show that percentile-normalization is a simple, non-parametric approach for correcting batch effects and improving sensitivity in case-control meta-analyses.},
archivePrefix = {arXiv},
arxivId = {1705.11170},
author = {Gibbons, Sean M. and Duvallet, Claire and Alm, Eric J.},
doi = {10.1371/journal.pcbi.1006102},
eprint = {1705.11170},
file = {:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Gibbons, Duvallet, Alm{\_}2018{\_}Correcting for batch effects in case-control microbiome studies.pdf:pdf;:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Gibbons, Duvallet, Alm{\_}2018{\_}Correcting for batch effects in case-control microbiome studies(2).pdf:pdf;:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Gibbons, Duvallet, Alm{\_}2018{\_}Correcting for batch effects in case-control microbiome studies(3).pdf:pdf},
isbn = {1336716436},
issn = {15537358},
journal = {PLoS Computational Biology},
pmid = {29684016},
title = {{Correcting for batch effects in case-control microbiome studies}},
year = {2018}
}
