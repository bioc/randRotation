file = {:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Li et al.{\_}2012{\_}Normalization, testing, and false discovery rate estimation for RNA-sequencing data.pdf:pdf},
