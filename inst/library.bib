journal = {Genome research},
number = {6},
pages = {991--8},
pmid = {21536721},
title = {{RNA-sequence analysis of human B-cells.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21536721 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3106332},
volume = {21},
year = {2011}
