doi = {10.1016/j.jcis.2004.05.042},
file = {:C$\backslash$:/Users/phett/Desktop/Skripten{\_}Ebooks/Mendeley{\_}bib/Liang, Mao, Ng{\_}2004{\_}Mechanical properties and stability measurement of cholesterol-containing liposome on mica by atomic force microscop.pdf:pdf},
